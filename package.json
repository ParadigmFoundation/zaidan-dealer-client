{
  "name": "zaidan-dealer-client",
  "version": "0.3.11",
  "main": "dist/index.js",
  "repository": "https://github.com/ParadigmFoundation/zaidan-dealer-client",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc",
    "test": "yarn start:snapshot && sleep 2 && yarn test:ci && yarn stop:snapshot",
    "test:ci": "ts-mocha --exit test/*_test.ts",
    "clean": "rm -rf node_modules dist",
    "compile:assets": "yarn build && webpack",
    "lint": "tslint -p .",
    "lint:fix": "tslint -p . --fix",
    "start:snapshot": "docker run -d --rm -p 8545:8545 --name zrx ${npm_package_config_image}",
    "stop:snapshot": "docker kill zrx",
    "dev": "webpack-dev-server",
    "prepublishOnly": "webpack",
    "docs": "typedoc ./src",
    "docs:dev": "vuepress dev docs",
    "docs:build": "yarn docs && vuepress build docs"
  },
  "config": {
    "image": "gcr.io/zaidan-eth-net/0x-snapshot:0.2.1"
  },
  "dependencies": {
    "0x.js": "^7.0.1",
    "@0x/contract-addresses": "^3.1.0",
    "@0x/subproviders": "^5.0.3",
    "@0x/web3-wrapper": "^6.0.12",
    "axios": "^0.19.0",
    "dotenv": "^6.2.0",
    "web3": "1.2.0"
  },
  "devDependencies": {
    "@0x/abi-gen-wrappers": "^5.0.3",
    "@0x/typescript-typings": "^4.2.4",
    "@types/mocha": "^5.2.7",
    "@types/web3": "^1.0.19",
    "@vuepress/plugin-google-analytics": "^1.0.0-alpha.0",
    "body-parser": "^1.19.0",
    "ethereum-types": "^2.1.4",
    "express": "^4.17.1",
    "mocha": "^6.2.0",
    "ts-loader": "^6.0.4",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-markdown": "^2.0.1",
    "typescript": "^3.5.3",
    "uuid": "^3.3.3",
    "vuepress": "^0.14.8",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.6.0",
    "webpack-dev-server": "^3.4.1"
  },
  "files": [
    "dist/*.js",
    "dist/*.js.map",
    "dist/*.d.ts",
    "webpack.config.js"
  ],
  "publishConfig": {
    "access": "public"
  }
}